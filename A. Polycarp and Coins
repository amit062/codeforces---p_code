https://codeforces.com/problemset/problem/1551/A

#include<bits/stdc++.h>
using namespace std;
int main(){
    int x;cin>>x;
    while(x--){
        int t;cin>>t;
    
        if(t%3==0){
            cout<<t/3<<" "<<t/3<<endl;
        }else if(t%3==1){
            cout<<t/3 +1<<"  "<<t/3<<endl;
        }else if(t%3==2){
            cout<<t/3<<" "<<t/3 +1<<endl;
        }
    }
}
